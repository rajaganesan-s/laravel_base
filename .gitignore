# Ignore dependencies and vendor folder
/vendor
/node_modules

# Ignore environment files
# Uncomment the next line if you need to push `.env` (not recommended in public repos)
/*.env.*.backup

# Ignore storage and cache (these can be regenerated)
# If you need to share specific storage files, keep them and ignore only the auto-generated ones
/storage/logs
/storage/framework/cache
/storage/framework/sessions
/storage/framework/testing
/storage/framework/views
/public/hot
/public/storage

# Laravel Mix (only compiled assets)
/public/mix-manifest.json
/public/css/*.css
/public/js/*.js
/public/mix

# Ignore IDE/editor specific files (unnecessary for sharing)
/.idea
.vscode
.phpstorm.meta.php
*.sublime-project
*.sublime-workspace

# Ignore debugging/testing files
/phpunit.result.cache
coverage/
*.log
npm-debug.log
yarn-error.log
pnpm-debug.log

# Ignore OS-specific files
.DS_Store
Thumbs.db
